<mvc:View controllerName="ns.businesspartners.controller.Suppliers"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List items="{/BusinessPartnerSet}">
                <items>
                    <StandardListItem type="Navigation" title="{CompanyName}" description="{WebAddress}"/>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
